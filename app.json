{
  "expo": {
    "name": "back2use-mobile",
    "slug": "back2use-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "com.back2use",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to scan QR codes for package borrowing."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-barcode-scanner"
    ],
    "extra": {
      "apiBaseUrl": "http://192.168.0.197:8000",
      "router": {},
      "eas": {
        "projectId": "463bb199-8a7c-4435-aa81-6781c18a49fe"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/463bb199-8a7c-4435-aa81-6781c18a49fe"
    }
  }
}
