{
  "expo": {
    "name": "back2use-mobile",
    "slug": "back2use-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "com.back2use",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to scan QR codes for package borrowing.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show nearby stores and your current location."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.back2use"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.duong2306.back2usemobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-barcode-scanner",
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "kotlinVersion": "2.0.20",
            "buildToolsVersion": "34.0.0",
            "compileSdkVersion": 34,
            "targetSdkVersion": 34
          }
        }
      ]
    ],
    "fonts": [
      "Poppins_400Regular",
      "Poppins_500Medium",
      "Poppins_600SemiBold",
      "Poppins_700Bold",
      "Poppins_800ExtraBold",
      "Poppins_900Black"
    ],
    "extra": {
      "apiBaseUrl": "https://back-2-use.up.railway.app",
      "router": {},
      "eas": {
        "projectId": "463bb199-8a7c-4435-aa81-6781c18a49fe"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "enabled": false
    }
  }
}